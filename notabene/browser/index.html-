            border-color: #770000;
            border-radius: 50%;
            padding-left: 20px;
            padding-right: 20px;
            visibility: hidden;
            background-image: url("halfsphere.jpg");
            background-size: 125%;
        }

        .content_titles .tagcloud {
            display: inline-block;
            margin: 20px;
        }

        .content_minds .tagcloud {
            display: inline-block;
            margin: 20px;
        }
    </style>
    <!-- custom style -->
