    <script>
        var gTc_titles;
        var gTc_minds;
        var gMindNo;
        var gRadius = 0;
        function recreateTitles() {
            if (gTc_titles)
                gTc_titles.destroy();
            gTc_titles = TagCloud('.content_titles', gTitles, {'radius' : gRadius});
        }
        function destroyMinds() {
            if (gTc_minds) {
                gTc_minds.destroy();
                gTc_minds = null;
            }
        }
        function recreateMinds() {
            destroyMinds();
            gTc_minds = TagCloud('.content_minds', gMinds[gMindNo], {'radius' : gRadius});
        }
        function changeRadius(radius) {
            if (gRadius == radius) return;
            gRadius = radius;
            if (gTc_titles) recreateTitles();
            if (gTc_minds)  recreateMinds();
        }
        function adoptToWidth() {
            var st_header=document.getElementsByClassName('header')[0].style;
            var st_content_box=document.getElementsByClassName('header')[0].style;
            if (document.documentElement.clientWidth < 630) { //>
                st_header.visibility = "hidden";
                st_header.width = '0';
                st_header.height = '0';
                changeRadius(130);
            } else {
                st_header.visibility = "visible";
                st_header.width = 'fit-content';
                st_header.height = '';
                changeRadius(250);
            }
        }
        function toResume() {
            if (gTc_titles) gTc_titles.resume();
            if (gTc_minds)  gTc_minds.resume();
        }
        function toPause() {
            if (gTc_titles) gTc_titles.pause();
            if (gTc_minds)  gTc_minds.pause();
        }
        function clickEventMinds(e) {
            if (e.target.className === 'tagcloud--item') {
                toPause();
            }
        }
        function addClickEventMinds() {
            var rootEl = document.querySelector('.content_minds');
            rootEl.addEventListener('click', clickEventMinds);
        }
        function clickEventTitles(e) {
            if (e.target.className === 'tagcloud--item') {
                adoptToWidth();
                var i;
                for (i = 0; i < gTitles.length && gTitles[i].localeCompare(e.target.innerText) != 0; ++i); //>
                gMindNo = i;
                recreateMinds();
                var st=document.getElementsByClassName('content_minds')[0].style
                st.visibility = "visible";
                st.marginLeft = "auto";
                st.marginRight = "auto";
                toResume();
            }
        }
        function addClickEventTitles() {
            var rootEl = document.querySelector('.content_titles');
            rootEl.addEventListener('click', clickEventTitles);
        }
        function onResize() {
            adoptToWidth();
        }
        function onLoad() {
            adoptToWidth();
            recreateTitles();
            addClickEventTitles();
            addClickEventMinds();
        }
    </script>
</head>

